    "def calculate_investment_roi(priority_df):\n",
    "    \"\"\"\n",
    "    Calculate return on investment for closing funding gaps.\n",
    "    \"\"\"\n",
    "    # Cost of closing gaps\n",
    "    total_gap = priority_df[priority_df['funding_gap'] > 0]['funding_gap'].sum()\n",
    "    \n",
    "    # Estimated benefits (conservative)\n",
    "    # Based on NIFC data: $1 in prevention saves $5-10 in suppression\n",
    "    suppression_savings_ratio = 5  # Conservative\n",
    "    \n",
    "    # Acres that would be better protected\n",
    "    underfunded = priority_df[priority_df['funding_gap'] > 0]\n",
    "    acres_improved = underfunded['acres'].sum()\n",
    "    population_protected = underfunded['population'].sum()\n",
    "    \n",
    "    # Estimated annual wildfire cost in study area\n",
    "    # Assume $500/acre average suppression cost for high-risk areas\n",
    "    high_risk_acres = priority_df[priority_df['risk_category'] == 'High']['acres'].sum()\n",
    "    estimated_annual_fire_cost = high_risk_acres * 0.01 * 500  # 1% burns annually at $500/acre\n",
    "    \n",
    "    roi_analysis = {\n",
    "        'investment_needed': total_gap,\n",
    "        'acres_improved': acres_improved,\n",
    "        'population_protected': population_protected,\n",
    "        'estimated_annual_savings': total_gap * suppression_savings_ratio,\n",
    "        'roi_ratio': suppression_savings_ratio,\n",
    "        'payback_period_years': 1 / suppression_savings_ratio,\n",
    "        'current_fire_cost_estimate': estimated_annual_fire_cost,\n",
    "        'potential_cost_reduction_pct': min(50, total_gap / estimated_annual_fire_cost * 100 * suppression_savings_ratio)\n",
    "    }\n",
    "    \n",
    "    return roi_analysis\n",
    "\n",
    "# Calculate ROI\n",
    "roi = calculate_investment_roi(priority_analysis)\n",
    "\n",
    "print(\"\\nROI ANALYSIS: CLOSING THE FUNDING GAP\")\n",
    "print(\"=\"*80)\n",
    "print(f\"\\nðŸ’µ INVESTMENT NEEDED:\")\n",
    "print(f\"   Additional funding required: ${roi['investment_needed']:,.0f}\")\n",
    "print(f\"\\nðŸŽ¯ IMPACT:\")\n",
    "print(f\"   Acres with improved protection: {roi['acres_improved']:,.0f}\")\n",
    "print(f\"   Population better protected: {roi['population_protected']:,}\")\n",
    "print(f\"\\nðŸ’° RETURN ON INVESTMENT:\")\n",
    "print(f\"   Estimated annual savings: ${roi['estimated_annual_savings']:,.0f}\")\n",
    "print(f\"   ROI ratio: {roi['roi_ratio']}:1\")\n",
    "print(f\"   â†’ Every $1 invested saves ${roi['roi_ratio']} in suppression costs\")\n",
    "print(f\"\\nâ±ï¸  PAYBACK:\")\n",
    "print(f\"   Payback period: {roi['payback_period_years']:.1f} years\")\n",
    "print(f\"\\nðŸ“‰ RISK REDUCTION:\")\n",
    "print(f\"   Current estimated fire costs: ${roi['current_fire_cost_estimate']:,.0f}/year\")\n",
    "print(f\"   Potential cost reduction: {roi['potential_cost_reduction_pct']:.1f}%\")"
