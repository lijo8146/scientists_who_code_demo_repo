    "def assign_fire_risk(tribal_lands_gdf):\n",
    "    \"\"\"\n",
    "    Assign fire risk scores to tribal lands.\n",
    "    \n",
    "    Real data sources:\n",
    "    - LANDFIRE Wildfire Hazard Potential\n",
    "    - State fire risk assessments\n",
    "    - Tribal fire management plans\n",
    "    \"\"\"\n",
    "    risk_data = tribal_lands_gdf.copy()\n",
    "    \n",
    "    # Assign risk scores (1-10) with some variation\n",
    "    # Larger reservations tend to have more diverse risk\n",
    "    np.random.seed(42)  # Reproducible\n",
    "    \n",
    "    risk_scores = []\n",
    "    for idx, tribe in risk_data.iterrows():\n",
    "        # Base risk varies by location\n",
    "        if tribe['state'] in ['OR', 'WA']:  # Pacific Northwest - high risk\n",
    "            base = 7\n",
    "        elif tribe['state'] in ['ID', 'MT']:  # Northern Rockies - very high risk\n",
    "            base = 8\n",
    "        else:\n",
    "            base = 6\n",
    "        \n",
    "        # Add variation\n",
    "        risk = np.clip(base + np.random.normal(0, 1), 1, 10)\n",
    "        risk_scores.append(risk)\n",
    "    \n",
    "    risk_data['fire_risk_score'] = risk_scores\n",
    "    \n",
    "    # Categorize\n",
    "    risk_data['risk_category'] = pd.cut(\n",
    "        risk_data['fire_risk_score'],\n",
    "        bins=[0, 4, 7, 10],\n",
    "        labels=['Low', 'Moderate', 'High']\n",
    "    )\n",
    "    \n",
    "    return risk_data\n",
    "\n",
    "# Assign fire risk\n",
    "tribal_lands = assign_fire_risk(tribal_lands)\n",
    "\n",
    "print(\"\\nFIRE RISK ASSIGNMENT\")\n",
    "print(\"=\"*80)\n",
    "print(\"Risk distribution:\")\n",
    "print(tribal_lands['risk_category'].value_counts())\n",
    "print(f\"\\nMean risk score: {tribal_lands['fire_risk_score'].mean():.2f}\")\n",
    "print(f\"High-risk reservations: {len(tribal_lands[tribal_lands['risk_category'] == 'High'])}\")"
   ]
