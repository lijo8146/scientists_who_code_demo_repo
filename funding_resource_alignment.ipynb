    "### 4.2 Identify High Risk / Low Investment Areas"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "def identify_priority_gaps(investment_df):\n",
    "    \"\"\"\n",
    "    Identify tribes with high risk but low investment.\n",
    "    These are priority areas for advocacy and increased funding.\n",
    "    \"\"\"\n",
    "    # Create risk vs. investment matrix\n",
    "    # High priority = High risk + Low investment\n",
    "    \n",
    "    # Normalize scores to 0-1\n",
    "    df = investment_df.copy()\n",
    "    df['risk_normalized'] = (df['fire_risk_score'] - df['fire_risk_score'].min()) / \\\n",
    "                           (df['fire_risk_score'].max() - df['fire_risk_score'].min())\n",
    "    df['investment_normalized'] = (df['investment_adequacy'] - df['investment_adequacy'].min()) / \\\n",
    "                                 (df['investment_adequacy'].max() - df['investment_adequacy'].min())\n",
    "    \n",
    "    # Priority score: High risk + Low investment\n",
    "    df['priority_score'] = df['risk_normalized'] * 0.6 + (1 - df['investment_normalized']) * 0.4\n",
    "    \n",
    "    # Categorize\n",
    "    df['priority_category'] = pd.cut(\n",
    "        df['priority_score'],\n",
    "        bins=[0, 0.33, 0.66, 1.0],\n",
    "        labels=['Lower Priority', 'Medium Priority', 'HIGH PRIORITY']\n",
    "    )\n",
    "    \n",
    "    # Identify quadrants\n",
    "    median_risk = df['fire_risk_score'].median()\n",
    "    median_investment = df['investment_adequacy'].median()\n",
    "    \n",
    "    df['quadrant'] = 'Unknown'\n",
    "    df.loc[\n",
    "        (df['fire_risk_score'] >= median_risk) & \n",
    "        (df['investment_adequacy'] < median_investment),\n",
    "        'quadrant'\n",
    "    ] = 'HIGH RISK / LOW INVESTMENT' \n",
    "    \n",
    "    df.loc[\n",
    "        (df['fire_risk_score'] >= median_risk) & \n",
    "        (df['investment_adequacy'] >= median_investment),\n",
    "        'quadrant'\n",
    "    ] = 'High Risk / Adequate Investment'\n",
    "    \n",
    "    df.loc[\n",
    "        (df['fire_risk_score'] < median_risk) & \n",
    "        (df['investment_adequacy'] < median_investment),\n",
    "        'quadrant'\n",
    "    ] = 'Lower Risk / Low Investment'\n",
    "    \n",
    "    df.loc[\n",
    "        (df['fire_risk_score'] < median_risk) & \n",
    "        (df['investment_adequacy'] >= median_investment),\n",
    "        'quadrant'\n",
    "    ] = 'Lower Risk / Adequate Investment'\n",
    "    \n",
    "    return df\n",
    "\n",
    "# Identify priority gaps\n",
    "priority_analysis = identify_priority_gaps(investment_analysis)\n",
    "\n",
    "print(\"\\nHIGH RISK / LOW INVESTMENT ANALYSIS\")\n",
    "print(\"=\"*80)\n",
    "print(\"\\nQuadrant Distribution:\")\n",
    "print(priority_analysis['quadrant'].value_counts())\n",
    "\n",
    "print(\"\\nPriority Category:\")\n",
    "print(priority_analysis['priority_category'].value_counts())\n",
    "\n",
    "# High priority tribes\n",
    "high_priority = priority_analysis[\n",
    "    priority_analysis['quadrant'] == 'HIGH RISK / LOW INVESTMENT'\n",
    "].sort_values('priority_score', ascending=False)\n",
    "\n",
    "print(f\"\\nâš ï¸  HIGH RISK / LOW INVESTMENT TRIBES: {len(high_priority)}\")\n",
    "print(\"\\nThese are PRIORITY areas for increased funding:\\n\")\n",
