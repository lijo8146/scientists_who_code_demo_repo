    "def calculate_disparity_metrics(investment_df):\n",
    "    \"\"\"\n",
    "    Calculate disparity metrics for policy advocacy.\n",
    "    \"\"\"\n",
    "    disparities = {}\n",
    "    \n",
    "    # Size-based disparities\n",
    "    large_tribes = investment_df[investment_df['acres'] > 500000]\n",
    "    small_tribes = investment_df[investment_df['acres'] <= 50000]\n",
    "    \n",
    "    disparities['large_vs_small_per_acre'] = {\n",
    "        'large_mean': large_tribes['funding_per_acre'].mean(),\n",
    "        'small_mean': small_tribes['funding_per_acre'].mean(),\n",
    "        'ratio': large_tribes['funding_per_acre'].mean() / small_tribes['funding_per_acre'].mean()\n",
    "    }\n",
    "    \n",
    "    # Risk-based disparities\n",
    "    high_risk = investment_df[investment_df['risk_category'] == 'High']\n",
    "    low_risk = investment_df[investment_df['risk_category'] == 'Low']\n",
    "    \n",
    "    if len(high_risk) > 0 and len(low_risk) > 0:\n",
    "        disparities['risk_investment_alignment'] = {\n",
    "            'high_risk_per_acre': high_risk['funding_per_acre'].mean(),\n",
    "            'low_risk_per_acre': low_risk['funding_per_acre'].mean(),\n",
    "            'ratio': high_risk['funding_per_acre'].mean() / low_risk['funding_per_acre'].mean(),\n",
    "            'aligned': high_risk['funding_per_acre'].mean() > low_risk['funding_per_acre'].mean()\n",
    "        }\n",
    "    \n",
    "    # Investment adequacy gaps\n",
    "    disparities['adequacy'] = {\n",
    "        'mean_adequacy': investment_df['investment_adequacy'].mean(),\n",
    "        'tribes_below_5': len(investment_df[investment_df['investment_adequacy'] < 5]),\n",
    "        'pct_below_5': len(investment_df[investment_df['investment_adequacy'] < 5]) / len(investment_df) * 100\n",
    "    }\n",
    "    \n",
    "    # Funding concentration\n",
    "    total_funding = investment_df['total_funding'].sum()\n",
    "    top_3_funding = investment_df.nlargest(3, 'total_funding')['total_funding'].sum()\n",
    "    \n",
    "    disparities['concentration'] = {\n",
    "        'top_3_pct': top_3_funding / total_funding * 100,\n",
    "        'gini_coefficient': calculate_gini(investment_df['total_funding'].values)\n",
    "    }\n",
    "    \n",
    "    return disparities\n",
    "\n",
    "def calculate_gini(values):\n",
    "    \"\"\"Calculate Gini coefficient (0=perfect equality, 1=perfect inequality)\"\"\"\n",
    "    sorted_values = np.sort(values)\n",
    "    n = len(values)\n",
    "    index = np.arange(1, n + 1)\n",
    "    return (2 * np.sum(index * sorted_values)) / (n * np.sum(sorted_values)) - (n + 1) / n\n",
    "\n",
    "# Calculate disparities\n",
    "disparities = calculate_disparity_metrics(priority_analysis)\n",
    "\n",
    "print(\"\\nFUNDING DISPARITY ANALYSIS\")\n",
    "print(\"=\"*80)\n",
    "print(\"\\nðŸ“Š KEY FINDINGS FOR POLICY ADVOCACY:\\n\")\n",
    "\n",
    "print(\"1. SIZE-BASED DISPARITY:\")\n",
    "lv_s = disparities['large_vs_small_per_acre']\n",
    "print(f\"   Large tribes (>500K acres): ${lv_s['large_mean']:.2f}/acre\")\n",
    "print(f\"   Small tribes (â‰¤50K acres): ${lv_s['small_mean']:.2f}/acre\")\n",
    "print(f\"   â†’ Large tribes receive {lv_s['ratio']:.1f}x more per acre\")\n",
    "\n",
    "if 'risk_investment_alignment' in disparities:\n",
    "    print(\"\\n2. RISK ALIGNMENT:\")\n",
    "    ria = disparities['risk_investment_alignment']\n",
    "    print(f\"   High-risk tribes: ${ria['high_risk_per_acre']:.2f}/acre\")\n",
    "    print(f\"   Low-risk tribes: ${ria['low_risk_per_acre']:.2f}/acre\")\n",
    "    if ria['aligned']:\n",
    "        print(f\"   âœ“ Investment aligned with risk ({ria['ratio']:.2f}x)\")\n",
    "    else:\n",
    "        print(f\"   âœ— Investment NOT aligned with risk\")\n",
    "\n",
    "print(\"\\n3. INVESTMENT ADEQUACY:\")\n",
    "adq = disparities['adequacy']\n",
    "print(f\"   Mean adequacy score: {adq['mean_adequacy']:.1f}/10\")\n",
    "print(f\"   Tribes severely underfunded (score <5): {adq['tribes_below_5']} ({adq['pct_below_5']:.1f}%)\")\n",
    "\n",
    "print(\"\\n4. FUNDING CONCENTRATION:\")\n",
    "conc = disparities['concentration']\n",
    "print(f\"   Top 3 tribes receive: {conc['top_3_pct']:.1f}% of all funding\")\n",
    "print(f\"   Gini coefficient: {conc['gini_coefficient']:.3f}\")\n",
    "print(f\"   (0=perfect equality, 1=maximum inequality)\")"
