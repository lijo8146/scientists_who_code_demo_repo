    "def analyze_funding_trends(funding_df, tribal_lands_gdf):\n",
    "    \"\"\"\n",
    "    Analyze multi-year funding trends.\n",
    "    \"\"\"\n",
    "    # Annual totals by tribe\n",
    "    annual_totals = funding_df.groupby(['tribe_id', 'year'])['amount'].sum().reset_index()\n",
    "    \n",
    "    # Add tribe info\n",
    "    annual_totals = annual_totals.merge(\n",
    "        tribal_lands_gdf[['tribe_id', 'tribe_name', 'acres']],\n",
    "        on='tribe_id'\n",
    "    )\n",
    "    \n",
    "    # Per-acre trends\n",
    "    annual_totals['amount_per_acre'] = annual_totals['amount'] / annual_totals['acres']\n",
    "    \n",
    "    # Calculate growth rates\n",
    "    trends = []\n",
    "    for tribe_id in annual_totals['tribe_id'].unique():\n",
    "        tribe_data = annual_totals[annual_totals['tribe_id'] == tribe_id].sort_values('year')\n",
    "        \n",
    "        if len(tribe_data) >= 2:\n",
    "            years = tribe_data['year'].values\n",
    "            amounts = tribe_data['amount'].values\n",
    "            \n",
    "            # Linear regression\n",
    "            slope, intercept, r_value, p_value, std_err = stats.linregress(years, amounts)\n",
    "            \n",
    "            # Calculate percent change\n",
    "            first_year_amount = tribe_data.iloc[0]['amount']\n",
    "            last_year_amount = tribe_data.iloc[-1]['amount']\n",
    "            pct_change = ((last_year_amount - first_year_amount) / first_year_amount) * 100\n",
    "            \n",
    "            trends.append({\n",
    "                'tribe_id': tribe_id,\n",
    "                'tribe_name': tribe_data.iloc[0]['tribe_name'],\n",
    "                'first_year': years[0],\n",
    "                'last_year': years[-1],\n",
    "                'first_year_amount': first_year_amount,\n",
    "                'last_year_amount': last_year_amount,\n",
    "                'pct_change': pct_change,\n",
    "                'annual_change': slope,\n",
    "                'trend': 'Increasing' if slope > 0 else 'Decreasing'\n",
    "            })\n",
    "    \n",
    "    trends_df = pd.DataFrame(trends)\n",
    "    \n",
    "    return annual_totals, trends_df\n",
    "\n",
    "# Analyze trends\n",
    "annual_data, trend_data = analyze_funding_trends(funding_awards, tribal_lands)\n",
    "\n",
    "print(\"\\nMULTI-YEAR FUNDING TRENDS\")\n",
    "print(\"=\"*80)\n",
    "print(f\"\\nOverall trends (2020-2024):\")\n",
    "print(f\"  Increasing: {len(trend_data[trend_data['trend'] == 'Increasing'])} tribes\")\n",
    "print(f\"  Decreasing: {len(trend_data[trend_data['trend'] == 'Decreasing'])} tribes\")\n",
    "\n",
    "print(f\"\\nBiggest gainers:\")\n",
    "display(trend_data.nlargest(3, 'pct_change')[[\n",
    "    'tribe_name', 'first_year_amount', 'last_year_amount', 'pct_change'\n",
    "]].round(2))\n",
    "\n",
    "print(f\"\\nBiggest losers:\")\n",
    "display(trend_data.nsmallest(3, 'pct_change')[[\n",
    "    'tribe_name', 'first_year_amount', 'last_year_amount', 'pct_change'\n",
    "]].round(2))"
