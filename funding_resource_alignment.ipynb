    "## 4. Funding Gap Analysis\n",
    "\n",
    "### 4.1 Calculate Investment Metrics"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "def calculate_investment_metrics(tribal_lands_gdf, funding_df, analysis_year=2024):\n",
    "    \"\"\"\n",
    "    Calculate per-acre, per-capita, and risk-adjusted investment metrics.\n",
    "    \"\"\"\n",
    "    # Filter to analysis year\n",
    "    year_funding = funding_df[funding_df['year'] == analysis_year].copy()\n",
    "    \n",
    "    # Aggregate by tribe\n",
    "    tribe_totals = year_funding.groupby('tribe_id')['amount'].sum().reset_index()\n",
    "    tribe_totals.columns = ['tribe_id', 'total_funding']\n",
    "    \n",
    "    # Merge with tribal data\n",
    "    analysis = tribal_lands_gdf.merge(tribe_totals, on='tribe_id', how='left')\n",
    "    analysis['total_funding'] = analysis['total_funding'].fillna(0)\n",
    "    \n",
    "    # Calculate metrics\n",
    "    analysis['funding_per_acre'] = analysis['total_funding'] / analysis['acres']\n",
    "    analysis['funding_per_capita'] = analysis['total_funding'] / analysis['population']\n",
    "    analysis['funding_per_wui_acre'] = analysis['total_funding'] / analysis['wui_acres']\n",
    "    \n",
    "    # Risk-adjusted investment\n",
    "    # What SHOULD they get based on risk? (hypothetical fair allocation)\n",
    "    total_risk_weighted_acres = (tribal_lands_gdf['acres'] * tribal_lands_gdf['fire_risk_score']).sum()\n",
    "    total_available = year_funding['amount'].sum()\n",
    "    \n",
    "    analysis['fair_share_estimate'] = (\n",
    "        (analysis['acres'] * analysis['fire_risk_score']) / \n",
    "        total_risk_weighted_acres * total_available\n",
    "    )\n",
    "    \n",
    "    analysis['funding_gap'] = analysis['fair_share_estimate'] - analysis['total_funding']\n",
    "    analysis['funding_gap_pct'] = (\n",
    "        analysis['funding_gap'] / analysis['fair_share_estimate'] * 100\n",
    "    )\n",
    "    \n",
    "    # Investment adequacy score (0-10)\n",
    "    # Based on funding relative to risk\n",
    "    expected_per_acre = analysis['fire_risk_score'] * 10  # $10/acre per risk point\n",
    "    analysis['investment_adequacy'] = (\n",
    "        analysis['funding_per_acre'] / expected_per_acre * 10\n",
    "    ).clip(0, 10)\n",
    "    \n",
    "    return analysis\n",
    "\n",
    "# Calculate metrics\n",
    "investment_analysis = calculate_investment_metrics(tribal_lands, funding_awards)\n",
    "\n",
    "print(\"\\nINVESTMENT METRICS (2024)\")\n",
    "print(\"=\"*80)\n",
    "print(f\"\\nTotal funding allocated: ${investment_analysis['total_funding'].sum():,.0f}\")\n",
    "print(f\"\\nPer-acre investment:\")\n",
    "print(f\"  Mean: ${investment_analysis['funding_per_acre'].mean():.2f}\")\n",
    "print(f\"  Median: ${investment_analysis['funding_per_acre'].median():.2f}\")\n",
    "print(f\"  Range: ${investment_analysis['funding_per_acre'].min():.2f} - ${investment_analysis['funding_per_acre'].max():.2f}\")\n",
    "\n",
    "print(f\"\\nPer-capita investment:\")\n",
    "print(f\"  Mean: ${investment_analysis['funding_per_capita'].mean():.2f}\")\n",
    "print(f\"  Median: ${investment_analysis['funding_per_capita'].median():.2f}\")\n",
    "\n",
    "print(f\"\\nFunding gaps:\")\n",
    "underfunded = investment_analysis[investment_analysis['funding_gap'] > 0]\n",
    "print(f\"  Underfunded tribes: {len(underfunded)}\")\n",
    "print(f\"  Total gap: ${underfunded['funding_gap'].sum():,.0f}\")\n",
    "\n",
    "display(investment_analysis[[\n",
    "    'tribe_name', 'fire_risk_score', 'total_funding', 'funding_per_acre',\n",
    "    'fair_share_estimate', 'funding_gap', 'investment_adequacy'\n",
    "]].sort_values('funding_gap', ascending=False).head(10).round(2))"
   ]
