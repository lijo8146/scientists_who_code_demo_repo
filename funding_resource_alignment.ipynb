    "def generate_funding_data(tribal_lands_gdf, years=[2020, 2021, 2022, 2023, 2024]):\n",
    "    \"\"\"\n",
    "    Generate simulated funding award data.\n",
    "    \n",
    "    Real data sources:\n",
    "    - USASpending.gov\n",
    "    - BIA financial reports\n",
    "    - USFS grant databases\n",
    "    - FEMA awards\n",
    "    - Tribal government records\n",
    "    \"\"\"\n",
    "    all_awards = []\n",
    "    \n",
    "    np.random.seed(42)\n",
    "    \n",
    "    for year in years:\n",
    "        for idx, tribe in tribal_lands_gdf.iterrows():\n",
    "            # BIA base allocation (all tribes get something)\n",
    "            bia_amount = (\n",
    "                tribe['acres'] * 0.5 +  # $0.50/acre\n",
    "                tribe['wui_acres'] * 5 +  # $5/WUI acre\n",
    "                np.random.normal(50000, 10000)  # Base + variation\n",
    "            )\n",
    "            bia_amount = max(50000, bia_amount)  # Minimum\n",
    "            \n",
    "            all_awards.append({\n",
    "                'tribe_id': tribe['tribe_id'],\n",
    "                'year': year,\n",
    "                'program': 'BIA_FIRE_PROGRAM',\n",
    "                'amount': bia_amount,\n",
    "                'award_type': 'base'\n",
    "            })\n",
    "            \n",
    "            # Competitive grants (not all tribes)\n",
    "            # Larger tribes more likely to win\n",
    "            # Capacity = acres + population\n",
    "            capacity_score = (tribe['acres'] / 100000) + (tribe['population'] / 1000)\n",
    "            \n",
    "            # NFP grant (30% chance, higher for larger tribes)\n",
    "            if np.random.random() < min(0.3 * capacity_score, 0.7):\n",
    "                nfp_amount = np.random.uniform(10000, 150000)\n",
    "                all_awards.append({\n",
    "                    'tribe_id': tribe['tribe_id'],\n",
    "                    'year': year,\n",
    "                    'program': 'NFP',\n",
    "                    'amount': nfp_amount,\n",
    "                    'award_type': 'competitive'\n",
    "                })\n",
    "            \n",
    "            # BIL funding (20% chance, recent years only)\n",
    "            if year >= 2022 and np.random.random() < min(0.2 * capacity_score, 0.5):\n",
    "                bil_amount = np.random.uniform(250000, 5000000)\n",
    "                all_awards.append({\n",
    "                    'tribe_id': tribe['tribe_id'],\n",
    "                    'year': year,\n",
    "                    'program': 'BIL_FIRE',\n",
    "                    'amount': bil_amount,\n",
    "                    'award_type': 'competitive'\n",
    "                })\n",
    "            \n",
    "            # IRA funding (10% chance, 2023+ only)\n",
    "            if year >= 2023 and np.random.random() < min(0.1 * capacity_score, 0.3):\n",
    "                ira_amount = np.random.uniform(500000, 10000000)\n",
    "                all_awards.append({\n",
    "                    'tribe_id': tribe['tribe_id'],\n",
    "                    'year': year,\n",
    "                    'program': 'IRA_FIRE',\n",
    "                    'amount': ira_amount,\n",
    "                    'award_type': 'competitive'\n",
    "                })\n",
    "    \n",
    "    return pd.DataFrame(all_awards)\n",
    "\n",
    "# Generate funding data\n",
    "funding_awards = generate_funding_data(tribal_lands)\n",
    "\n",
    "print(\"\\nFUNDING AWARDS DATA\")\n",
    "print(\"=\"*80)\n",
    "print(f\"Total awards: {len(funding_awards):,}\")\n",
    "print(f\"Total funding: ${funding_awards['amount'].sum():,.0f}\")\n",
    "print(f\"\\nBy program:\")\n",
    "program_summary = funding_awards.groupby('program')['amount'].agg(['count', 'sum'])\n",
    "program_summary.columns = ['Count', 'Total $']\n",
    "program_summary['Total $'] = program_summary['Total $'].apply(lambda x: f\"${x:,.0f}\")\n",
    "display(program_summary)\n",
    "\n",
    "print(f\"\\nBy year:\")\n",
    "year_summary = funding_awards.groupby('year')['amount'].sum()\n",
    "for year, total in year_summary.items():\n",
    "    print(f\"  {year}: ${total:,.0f}\")"
   ]
